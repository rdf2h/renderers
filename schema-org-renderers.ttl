<Article>  a r2h:Renderer;
    r2h:type s:Article;
    r2h:context r2h:Default;
    r2h:mustache """
      {{{@prefix s: <http://schema.org/>}}}
      <h1>
      {{s:headline}}
      {{^+s:headline}}
        Article: <a href='{{.}}'>
          {{{:render . s:name}}}
        </a>
      {{/+s:headline}}
      </h1>

      {{{:render . s:Article}}}
    """.

<CreativeWork>  a r2h:Renderer;
    r2h:type s:CreativeWork;
    r2h:context r2h:Default;
    r2h:mustache """
      {{{@prefix s: <http://schema.org/>}}}
      <h1>
      {{s:headline}}
      {{^+s:headline}}
        Creative Work: <a href='{{.}}'>
          {{{:render . s:name}}}
        </a>
      {{/+s:headline}}
      </h1>

      <h3>It is
      {{{:render . s:Article}}}
    """.


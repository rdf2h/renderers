<Person> a r2h:Renderer;
    r2h:type s:Person;
    r2h:context r2h:Default;
    r2h:mustache """
      {{{@prefix s: <http://schema.org/>}}}
      <h1>Person: <a href='{{.}}'>
        {{{:render . s:name}}}
      </a></h1>

      {{#+s:birthDate}}
        * {{s:birthDate}}
        {{#+s:deathDate}}
          -
        {{/+s:deathDate}}
      {{/+s:birthDate}}
      {{#+s:deathDate}}
        &#x271D
        {{s:deathDate}}
      {{/+s:deathDate}}
      
      {{#s:gender}}
        <br>{{.}}
      {{/s:gender}}

      {{#+s:spouse}}
        <h3>Married to:</h3>
        {{#s:spouse}}
        <a href='{{.}}'>
          {{{:render . s:name}}}
        </a><br>
        {{/s:spouse}}
      {{/+s:spouse}}

      {{#+s:parent}}
        <h3>Parents:</h3>
        {{#s:parent}}
          <a href='{{.}}'>
            {{{:render . s:name}}}
          </a><br>
        {{/s:parent}}
      {{/+s:parent}}
      
      {{#+s:children}}
        <h3>Children:</h3>
        {{#s:children}}
        <a href='{{.}}'>
          {{{:render . s:name}}}
        </a><br>
        {{/s:children}}
      {{/+s:children}}

      {{#+s:sibling}}
        <h3>Siblings:</h3>
        {{#s:sibling}}
        <a href='{{.}}'>
          {{{:render . s:name}}}
        </a><br>
        {{/s:sibling}}
      {{/+s:sibling}}

      {{#+s:relatedTo}}
        <h3>Related to:</h3>
        {{#s:relatedTo}}
        <a href='{{.}}'>
          {{{:render . s:name}}}
        </a><br>
        {{/s:relatedTo}}
      {{/+s:relatedTo}}

      {{#+s:address}}
        <h3>Address:</h3>
        {{#s:address}}
          {{{:render . s:address}}}
        {{/s:address}}
      {{/+s:address}}

      {{#+s:telephone}}
        <h3>Telephone:</h3>
        {{s:telephone}}
      {{/+s:telephone}}

      {{#+s:email}}
        <h3>Email:</h3>
        <a href='{{s:email}}'>{{s:email}}<a>
      {{/+s:email}}

      {{#+s:jobTitle}}
        <h3>Job title:</h3>
        {{s:jobTitle}}
      {{/+s:jobTitle}}

      {{#+s:author<-}}
        <h3>Author of:</h3>
        {{#s:author<-}}
        <div>
          {{{:render . r2h:Default}}}
        </div>
        {{/s:author<-}}
      {{/+s:author<-}}

      {{#+s:creator<-}}
        <h3>Creator of:</h3>
        {{#s:creator<-}}
        <div>
          {{{:render . r2h:Default}}}
        </div>
        {{/s:creator<-}}
      {{/+s:creator<-}}

      {{#+s:knows}}
        <h3>Knows:</h3>
        {{#s:knows}}
        <a href='{{.}}'>
          {{{:render . s:name}}}
        </a><br>
        {{/s:knows}}
      {{/+s:knows}}

      {{#+s:knows<-}}
        <h3>Known by:</h3>
        {{#s:knows<-}}
        <a href='{{.}}'>
            {{{:render . s:name}}}
          </a><br>
        {{/s:knows<-}}
      {{/+s:knows<-}}

      {{#+s:colleague}}
        <h3>Colleagues:</h3>
        {{#s:colleague}}
        <a href='{{.}}'>
            {{{:render . s:name}}}
          </a><br>
        {{/s:colleague}}
      {{/+s:colleague}}

      {{#+s:colleague<-}}
        <h3>Colleague of:</h3>
        {{#s:colleague<-}}
        <a href='{{.}}'>
            {{{:render . s:name}}}
          </a><br>
        {{/s:colleague<-}}
      {{/+s:colleague<-}}

      {{#+s:follows}}
        <h3>Follows:</h3>
        {{#s:follows}}
        <a href='{{.}}'>
            {{{:render . s:name}}}
          </a><br>
        {{/s:follows}}
      {{/+s:follows}}

      {{#+s:follows<-}}
        <h3>Followed by:</h3>
        {{#s:follows<-}}
        <a href='{{.}}'>
            {{{:render . s:name}}}
          </a><br>
        {{/s:follows<-}}
      {{/+s:follows<-}}

      {{{:render . s:Thing}}}
      """.


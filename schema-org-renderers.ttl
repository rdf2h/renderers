<name>  a r2h:Renderer;
    r2h:type rdfs:Resource;
    r2h:context s:name;
    r2h:mustache """
        {{s:name}}
        {{s:givenName}}
        {{s:additionalName}}
        {{s:familyName}}
        {{^+s:name}}
          {{^+s:givenName}}
            {{^+s:additionalName}}
              {{^+s:familyName}}
                {{.}}
              {{/+s:familyName}}
            {{/+s:additionalName}}
          {{/+s:givenName}}
        {{/+s:name}}
        {{#+s:brand}}
          ({{s:brand}})
        {{/+s:brand}}
        {{^+s:brand}}
          {{#+s:legalName}}
            ({{s:legalName}})
          {{/+s:legalName}}
        {{/+s:brand}}
    """.

